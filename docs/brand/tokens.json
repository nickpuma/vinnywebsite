{
    "$schema": "https://json.schemastore.org/style-dictionary.json",
    "name": "vnny",
    "version": "1.0.0",
    "colors": {
      "primary": {
        "sageDark": "#3D4F3F",
        "sage": "#4A5D4A",
        "sageMedium": "#5A6B52",
        "sageLight": "#6B7D63"
      },
      "background": {
        "white": "#FFFFFF",
        "cream": "#F5F5EB",
        "darkSage": "#3D4F3F"
      },
      "text": {
        "primary": "#3D4F3F",
        "secondary": "#5A6B5A",
        "muted": "#8B9B8B",
        "onDark": "#F5F5EB"
      },
      "accent": {
        "tealSlate": "#4A6B6B",
        "tealSlateLight": "#6B8B8B",
        "forest": "#4A6B4A",
        "ochre": "#B5864A",
        "slate": "#5A6B6B"
      }
    },
    "typography": {
      "fontFamily": {
        "sans": "Inter, ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\"",
        "serif": "ui-serif, Georgia, \"Times New Roman\", Times, serif"
      },
      "weights": {
        "regular": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700
      },
      "sizes": {
        "xs": 12,
        "sm": 14,
        "md": 16,
        "lg": 18,
        "xl": 22,
        "2xl": 28
      },
      "lineHeights": {
        "tight": 1.15,
        "normal": 1.4,
        "relaxed": 1.6
      }
    },
    "radii": {
      "sm": 10,
      "md": 14,
      "lg": 18,
      "xl": 22
    },
    "spacing": {
      "xs": 8,
      "sm": 12,
      "md": 16,
      "lg": 24,
      "xl": 32,
      "2xl": 48
    },
    "effects": {
      "shadowSoft": "0 10px 22px rgba(0,0,0,0.18)",
      "shadowCard": "0 12px 32px rgba(25, 39, 25, 0.12)"
    }
  }
  
  